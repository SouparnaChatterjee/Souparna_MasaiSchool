function personInfo() {
    console.log(`Name: ${this.name}, Age: ${this.age}`);
}
const person1 = {
    name: "John",
    age: 30
};
const person2 = {
    name: "Alice",
    age: 25
};
personInfo.call(person1); // Output: Name: John, Age: 30
personInfo.call(person2); // Output: Name: Alice, Age: 25
