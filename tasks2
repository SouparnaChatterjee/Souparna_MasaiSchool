tasks = ["Study", "Exercise", "Read", "Cook", "Sleep"];
for(let i = 0; i < tasks.length - 1; i++) {
    tasks[i] = tasks[i + 1];
}
tasks.length--;
for(let i = tasks.length - 1; i >= 0; i--) {
    tasks[i + 2] = tasks[i];
}
tasks[0] = "Meeting";
tasks[1] = "Project";
tasks[tasks.length - 1] = "Review";
console.log(tasks);
